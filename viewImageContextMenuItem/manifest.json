{
  "manifest_version": 2,
  "name": "View Image Context Menu Item",
  "version": "3.2",
  "description": "Adds View Audio, View Image, and View Video to the context menu",
  "default_locale": "en-CA",
  "icons": {
    "64": "icons/viewImageContextMenuItem-64.png",
    "48": "icons/viewImageContextMenuItem-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": ["menus", "<all_urls>", "webRequest", "webRequestBlocking", "storage"]
}